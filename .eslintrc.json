{
  "extends": "plugin:@typescript-eslint/recommended", // Здесь вы указываете, что наследуетесь от набора правил
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": 2
    }
}
